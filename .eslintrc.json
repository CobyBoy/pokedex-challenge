{
    "parserOptions": {
    "ecmaVersion":11,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
   
    "rules": {
        "semi": 2,
        "prefer-destructuring": ["error", {
        "array": true,
        "object": true
        }],
        "quotes": ["warn", "single"]
    }
}